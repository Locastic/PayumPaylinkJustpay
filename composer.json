{
    "name": "locastic/paylink-payum",
    "type": "library",
    "description": "Paylink Justpay integration with Payum",
    "keyword": ["paylink", "justpay", "payum", "symfony", "payment"],
    "homepage": "https://github.com/locastic/PayumPaylinkJustpay",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "payum/core": "~0.12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Locastic\\PaylinkPayum": "src/",
            "PayLink": "src/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "0.12-dev"
        }
    }
}
